  "description": "handy img tool and API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "bin": {
    "handy-img": "./dist/bin.mjs",
    "himg": "./dist/bin.mjs"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
