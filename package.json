  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/handy-img/issues"
  },
  "homepage": "https://github.com/magicdawn/handy-img#readme",
  "dependencies": {
    "@magicdawn/finder-sort": "^0.4.0",
    "@magicdawn/x-args": "^1.5.4",
    "@vingle/bmp-js": "^0.2.5",
    "bytes": "^3.1.2",
    "chalk": "^5.3.0",
    "clipanion": "^3.2.1",
    "esm-utils": "^4.3.0",
    "exifr": "^7.1.3",
    "fast-glob": "^3.3.2",
    "figures": "^6.1.0",
    "fs-extra": "^11.2.0",
    "humanize-duration": "^3.32.1",
    "log-symbols": "^6.0.0",
    "mac-helper": "^0.1.5",
    "node-mozjpeg": "^1.0.1",
    "os-locale": "^6.0.2",
    "promise.map": "^0.5.0",
    "sharp": "^0.33.4",
    "typanion": "^3.14.0"
  },
  "devDependencies": {
    "@magicdawn/eslint-config": "^0.1.0",
