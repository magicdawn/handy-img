{
  "name": "handy-img",
  "version": "0.10.0",
  "description": "handy img tool and API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "bin": {
    "handy-img": "./dist/bin.mjs",
    "himg": "./dist/bin.mjs"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test-cover": "vitest run --coverage",
    "report-cover": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/handy-img.git"
  },
  "keywords": [
    "handy-img",
    "img",
    "sharp",
    "resize",
    "compress",
    "mozjpeg"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/handy-img/issues"
  },
  "homepage": "https://github.com/magicdawn/handy-img#readme",
  "dependencies": {
    "@magicdawn/finder-sort": "^0.4.0",
    "@magicdawn/x-args": "^1.6.1",
    "@vingle/bmp-js": "^0.2.5",
    "bytes": "^3.1.2",
    "chalk": "^5.4.1",
    "clipanion": "^3.2.1",
    "esm-utils": "^4.4.2",
    "exifr": "^7.1.3",
    "fast-glob": "^3.3.3",
    "figures": "^6.1.0",
    "fs-extra": "^11.3.0",
    "humanize-duration": "^3.32.1",
    "log-symbols": "^7.0.0",
    "mac-helper": "^0.2.0",
    "micromatch": "^4.0.8",
    "node-mozjpeg": "^1.0.1",
    "os-locale": "^6.0.2",
    "path-type": "^6.0.0",
    "promise.map": "^1.1.2",
    "sharp": "^0.34.1",
    "typanion": "^3.14.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@magicdawn/eslint-config": "^0.1.0",
    "@magicdawn/prettier-config": "^0.1.0",
    "@swc/core": "^1.11.24",
    "@swc/helpers": "^0.5.17",
    "@types/bytes": "^3.1.5",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/humanize-duration": "^3.27.4",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.15.17",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "eslint": "9.26.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx,json,yml,md}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
